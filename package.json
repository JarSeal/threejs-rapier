{
  "name": "threejs-rapier",
  "version": "1.0.0",
  "description": "Project template for Three.js and Rapier",
  "main": "dist/index.html",
  "repository": "https://github.com/JarSeal/threerapier.git",
  "author": "JarSeal",
  "license": "MIT",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "VITE_APP_ENV=development vite serve --port 8080 --host",
    "dev:test": "VITE_APP_ENV=test vite serve --port 8080 --host",
    "dev:production": "vite serve --port 8080 --host",
    "lint": "eslint",
    "build": "tsc && vite build",
    "build:test": "tsc && VITE_APP_ENV=test vite build"
  },
  "dependencies": {
    "@dimforge/rapier3d-compat": "0.17.1",
    "stats-gl": "3.6.0",
    "three": "0.176.0",
    "tweakpane": "4.0.5"
  },
  "devDependencies": {
    "@tweakpane/core": "2.0.5",
    "@types/node": "22.10.6",
    "@types/three": "0.176.0",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.8.0",
    "eslint": "9.2.0",
    "eslint-config-import": "0.13.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "prettier": "3.2.5",
    "sass": "1.82.0",
    "typescript": "5.4.5",
    "vite": "5.2.11",
    "vite-plugin-wasm": "3.4.1"
  },
  "engines": {
    "node": ">=22.1.0",
    "yarn": ">=1.22.15"
  }
}
